{
  "name": "api",
  "version": "1.0.0",
  "description": "Do You Part Social Network API",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "prettier"
    ],
    "src/**/*.ts": "eslint"
  },
  "scripts": {
    "dev": "tsnd --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CristianodePaula/Do_You_Part-api.git"
  },
  "keywords": [
    "SocialNetwork",
    "ONG",
    "SocialActions"
  ],
  "author": "Cristiano de Paula Moreira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CristianodePaula/Do_You_Part-api/issues"
  },
  "homepage": "https://github.com/CristianodePaula/Do_You_Part-api#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.7",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  }
}
